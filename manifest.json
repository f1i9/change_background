{
    "manifest_version": 3,
    "name": "Change Background Extension",
    "description": "Change the background color of the active tab.",
    "version": "1.0",
    "action": {
      "default_popup": "./popup/popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ]
  }
  